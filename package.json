{
  "name": "@relaybox/client",
  "version": "1.0.0",
  "description": "A robust and scalable realtime infrastructure service with comprehensive WebSocket support, designed to facilitate live updates, event-driven communication, and seamless data streaming. Ideal for building realtime applications such as chat, multiplayer games, collaboration tools, push notifications, and more. This library provides the client-side programming API, providing a high-performance, low-latency, and reliable framework for all your realtime needs. Supports Node.js, JavaScript, and TypeScript.",
  "keywords": [
    "realtime infrastructure",
    "realtime web applications",
    "mobile realtime apps",
    "realtime platform",
    "web and mobile integration",
    "scalable realtime",
    "low latency websocket",
    "realtime communication",
    "event-driven architecture",
    "websocket library",
    "realtime API",
    "Node.js WebSocket",
    "realtime notifications",
    "realtime multiplayer",
    "realtime collaboration"
  ],
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c rollup.config.mjs",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "dev": "tsc --watch && rollup -c rollup.config.mjs --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relaybox/relaybox-client-js.git"
  },
  "author": "admin@relaybox.net",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/relaybox/relaybox-client-js/issues"
  },
  "homepage": "https://github.com/relaybox/relaybox-client-js#readme",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/node": "^20.14.10",
    "@types/node-fetch": "^2.6.11",
    "@types/vitest": "^0.24.2",
    "rollup": "^4.18.1",
    "typescript": "^5.5.3",
    "vitest": "^0.24.2"
  }
}
