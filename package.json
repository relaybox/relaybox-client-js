{
  "name": "@relaybox/client",
  "version": "1.0.9",
  "description": "RelayBox Client Library SDK. Designed to enable seamless connection and interaction with RelayBox's realtime services from client-side applications. It offers features such as real-time presence tracking, metrics collection, event-based publish/subscribe (pub/sub) capabilities, and comprehensive management of WebSocket connection lifecycles.",
  "keywords": [
    "realtime infrastructure",
    "realtime web applications",
    "mobile realtime apps",
    "realtime platform",
    "web and mobile integration",
    "scalable realtime",
    "low latency websocket",
    "realtime communication",
    "event-driven architecture",
    "websocket library",
    "realtime API",
    "realtime notifications",
    "realtime multiplayer",
    "realtime collaboration",
    "nodejs push notifications",
    "nodejs websocket",
    "nodejs realtime api",
    "server sent events",
    "uwebsockets"
  ],
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && rollup -c rollup.config.mjs",
    "publish": "npm run build && npm publish --access public",
    "test": "vitest",
    "dev": "tsc --watch && rollup -c rollup.config.mjs --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relaybox/relaybox-client-js.git"
  },
  "author": "support@relaybox.net",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relaybox/relaybox-client-js/issues"
  },
  "homepage": "https://github.com/relaybox/relaybox-client-js#readme",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.14.10",
    "@types/node-fetch": "^2.6.11",
    "dotenv": "^16.4.5",
    "msw": "^2.3.4",
    "rollup": "^4.19.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  }
}
